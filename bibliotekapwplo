1. CREATE TABLE bibliotekapwplo.`autor` ( IdAutor VARCHAR(30) NOT NULL , Imie VARCHAR(30) NOT NULL , Nazwisko VARCHAR(30) NOT NULL , Pseudonim VARCHAR(30) NOT NULL , PRIMARY KEY (IdAutor))
1.1 ALTER TABLE autor MODIFY IdAutor INT
2.CREATE TABLE bibliotekapwplo.`ksiazka` ( IdBook INT NOT NULL , IdAutor INT NOT NULL , Gatunek VARCHAR(40) NOT NULL , Opis VARCHAR(255) NOT NULL , Recenzja VARCHAR(255) NOT NULL , RokWydania DATE NOT NULL , Wydawnictwo VARCHAR(30) NOT NULL , Status INT NOT NULL , PRIMARY KEY (IdBook), FOREIGN KEY (IdAutor) REFERENCES autor (IdAutor) )
3.CREATE TABLE bibliotekapwplo.`gatunek` ( IdGatunek INT NOT NULL , PokazUkryj BOOLEAN NOT NULL , PRIMARY KEY (IdGatunek))
4.CREATE TABLE bibliotekapwplo.`recenzja` ( IdRecenzja INT NOT NULL , Ocena INT NOT NULL , Tresc VARCHAR(200) NOT NULL , UserName VARCHAR(30) NOT NULL , Ukryj BOOLEAN NOT NULL , PRIMARY KEY (IdRecenzja)
5.CREATE TABLE bibliotekapwplo.`wydawnictwo` ( IdWydawnictwo INT NOT NULL , Nazwa VARCHAR(30) NOT NULL , Status VARCHAR(30) NOT NULL , PRIMARY KEY (IdWydawnictwo))
6.CREATE TABLE bibliotekapwplo.`egzemplarz` ( IdEgzemplarz INT NOT NULL , IdBook INT NOT NULL , Status VARCHAR(30) NOT NULL , IdBiblioteka INT NOT NULL , Zużycie[0-100%] VARCHAR(30) NOT NULL , PRIMARY KEY (IdEgzemplarz)
7.CREATE TABLE bibliotekapwplo.`biblioteka` ( IdBiblioteka INT NOT NULL , IdLokalizacja INT NOT NULL , IdGodzinyOtwarcia INT NOT NULL , Nazwa VARCHAR(30) NOT NULL , Field VARCHAR(30) NOT NULL , PRIMARY KEY (IdBiblioteka))
8. CREATE TABLE bibliotekapwplo.`lokalizacja` ( IdLokalizacja INT NOT NULL , Miasto VARCHAR(30) NOT NULL , KodPocztowy VARCHAR(30) NOT NULL , Województwo VARCHAR(30) NOT NULL , PRIMARY KEY (IdLokalizacja))
9.CREATE TABLE bibliotekapwplo.`godzinyotwarcia` ( IdBiblioteka INT NOT NULL , IdGodzinyOtwarcia INT NOT NULL , DzienTygodnia VARCHAR(30) NOT NULL , Godziny VARCHAR(30) NOT NULL , Status VARCHAR(30) NOT NULL , PRIMARY KEY (IdGodzinyOtwarcia))
10.CREATE TABLE bibliotekapwplo.`uzytkownik` ( IdUzytkownik INT NOT NULL , Status VARCHAR(30) NOT NULL , Login VARCHAR(30) NOT NULL , Typ VARCHAR(30) NOT NULL , Nazwisko VARCHAR(30) NOT NULL , Imie VARCHAR(30) NOT NULL , IndeksPW INT NOT NULL , Wypozyczenie INT NOT NULL , Sms VARCHAR(30) NOT NULL , Mail VARCHAR(30) NOT NULL , PRIMARY KEY (IdUzytkownik))
11.CREATE TABLE bibliotekapwplo.`config` ( IdConfig INT NOT NULL , Nazwa VARCHAR(30) NOT NULL , Wartosc INT NOT NULL , DataModyfikacji DATE NOT NULL , DataDodania DATE NOT NULL )
12.CREATE TABLE bibliotekapwplo.`wypozyczenie` ( IdWyporzyczenie INT NOT NULL , IdZamowienie INT NOT NULL , IdEgzemplarz INT NOT NULL , DataWypozyczenia DATE NOT NULL , CzasWypozyczenia VARCHAR(30) NOT NULL , DataZwrotu DATE NOT NULL , Przedluzenie DATE NOT NULL , PRIMARY KEY (IdWyporzyczenie))
13.CREATE TABLE bibliotekapwplo.`notyfikacja` ( IdNotyfikacja INT NOT NULL , IdPrzedluzenie INT NOT NULL , Tresc VARCHAR(30) NOT NULL , Typ VARCHAR(30) NOT NULL , IdUzytkownik INT NOT NULL , PRIMARY KEY (IdNotyfikacja))
